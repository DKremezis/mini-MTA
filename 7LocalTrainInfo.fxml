                        <DropShadow />
                     </effect>
                     <font>
                        <Font name="System Italic" size="11.0" />
                     </font></TextField>
                  <TextArea fx:id="resultArea" editable="false" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="2">
                     <effect>
                        <DropShadow />
                     </effect>
